{
  "name": "monzolib",
  "version": "0.1.0",
  "description": "Fully Featured JS/Node Monzo Library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "homepage": "https://github.com/robjtede/monzolib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robjtede/monzolib.git"
  },
  "author": "robjtede",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "build": "npm run clean && tsc",
    "build:w": "npm run build -- --watch",
    "test": "npm run build && ava",
    "test:w": "npm run test -- --watch",
    "precommit": "lint-staged && npm test",
    "prepare": "npm run build"
  },
  "bugs": {
    "url": "https://github.com/robjtede/monzolib/issues"
  },
  "keywords": [
    "monzo",
    "bank",
    "fintech",
    "monzolib"
  ],
  "dependencies": {
    "common-tags": "^1.8.0",
    "date-fns": "^2.0.0-alpha.10",
    "debug": "^3.1.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@types/common-tags": "^1.4.0",
    "@types/debug": "0.0.30",
    "@types/lodash": "^4.14.116",
    "ava": "^0.25.0",
    "copyfiles": "^2.0.0",
    "eclint": "^2.8.0",
    "husky": "^0.14.3",
    "json-types": "^1.0.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.2",
    "tslib": "^1.9.3",
    "typescript": "^3.0.1"
  },
  "lint-staged": {
    "!*.{ts,js}": [
      "eclint fix",
      "git add"
    ],
    "*.{ts,js}": [
      "eclint fix",
      "prettier --write --no-semi --single-quote",
      "git add"
    ]
  }
}
